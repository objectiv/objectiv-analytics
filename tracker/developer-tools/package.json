{
  "name": "@objectiv/developer-tools",
  "version": "0.0.1",
  "description": "Objectiv Tracker Developer Tools",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "fixtures-app:start": "(cd tests/cra-app-for-fixtures && yarn start)",
    "fixtures-app:build": "(cd tests/cra-app-for-fixtures && yarn build)",
    "build": "rollup -c --silent --failAfterWarnings",
    "build:dev": "dev=true run build",
    "prettier": "prettier --write .",
    "tsc": "tsc --noEmit",
    "test": "jest --silent --runInBand",
    "test:ci": "jest --silent --ci --runInBand",
    "test:coverage": "jest --silent --coverage --runInBand"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "^2.0.1",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-standard-reporter": "^2.0.0",
    "prettier": "^2.3.0",
    "rollup": "^2.48.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
}
