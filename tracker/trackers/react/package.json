{
  "name": "@objectiv/tracker-react",
  "version": "0.0.1",
  "description": "Objectiv React application analytics tracker for the open taxonomy for analytics",
  "license": "Apache-2.0",
  "homepage": "https://objectiv.io",
  "keywords": [
    "objectiv",
    "tracking",
    "react",
    "analytics",
    "events",
    "taxonomy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/objectiv/objectiv-analytics.git",
    "directory": "tracker/trackers/react"
  },
  "bugs": "https://github.com/objectiv/objectiv-analytics/issues",
  "contributors": [
    {
      "name": "Surai Di Rosa",
      "email": "surai.dirosa@gmail.com",
      "url": "https://github.com/sdirosa"
    }
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --silent --failAfterWarnings",
    "prettify": "prettier --write .",
    "tsc": "tsc --noEmit",
    "test": "jest --silent --runInBand",
    "test:ci": "jest --silent --ci --runInBand",
    "test:coverage": "jest --silent --coverage --runInBand",
    "check:dependencies": "npx depcheck",
    "npm-publish": "shx rm -f .npmrc && npm publish --access=public",
    "npm-publish:verdaccio": "shx cp ../../verdaccio/.npmrc .npmrc && npm publish && shx rm -f .npmrc"
  },
  "devDependencies": {
    "@objectiv/testing-tools": "0.0.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/jest": "^27.0.2",
    "depcheck": "^1.4.2",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-standard-reporter": "^2.0.0",
    "jest-useragent-mock": "^0.1.1",
    "prettier": "^2.4.1",
    "rollup": "^2.58.3",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.7",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@objectiv/queue-local-storage": "^0.0.1",
    "@objectiv/tracker-core": "~0.0.1",
    "@objectiv/tracker-core-react": "~0.0.1",
    "@objectiv/transport-fetch": "^0.0.1",
    "@objectiv/transport-xhr": "^0.0.1"
  }
}
