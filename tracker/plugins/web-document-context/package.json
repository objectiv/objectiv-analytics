{
  "name": "@objectiv-analytics/plugin-web-document-context",
  "version": "0.0.1",
  "description": "Plugin for Objectiv trackers to automatically attach WebDocumentContexts",
  "license": "Apache-2.0",
  "homepage": "https://objectiv.io",
  "keywords": [
    "objectiv",
    "tracking",
    "web",
    "analytics",
    "events",
    "taxonomy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/objectiv/objectiv-analytics.git",
    "directory": "tracker/plugins/web-document-context"
  },
  "bugs": "https://github.com/objectiv/objectiv-analytics/issues",
  "contributors": [
    {
      "name": "Surai Di Rosa",
      "email": "surai.dirosa@gmail.com",
      "url": "https://github.com/sdirosa"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --silent --failAfterWarnings",
    "prettify": "prettier --write .",
    "tsc": "tsc --noEmit",
    "test": "jest --silent --runInBand",
    "test:ci": "jest --silent --ci --runInBand",
    "test:coverage": "jest --silent --coverage --runInBand",
    "generate:badges": "node ../../core/utilities/src/badges.js",
    "check:dependencies": "npx depcheck",
    "npm-publish": "shx rm -f .npmrc && npm publish",
    "npm-publish:verdaccio": "shx cp ../../verdaccio/.npmrc .npmrc && npm publish && shx rm -f .npmrc"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/jest": "^27.0.2",
    "depcheck": "^1.4.2",
    "jest": "^27.3.1",
    "jest-extended": "^1.1.0",
    "jest-standard-reporter": "^2.0.0",
    "prettier": "^2.4.1",
    "rollup": "^2.58.3",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.7",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@objectiv-analytics/tracker-core": "^0.0.1"
  }
}
